{
  "name": "longmo-stagewise-plugin",
  "version": "1.0.0",
  "description": "Web page fine-tuning plugin with element selector functionality",
  "type": "module",
  "main": "dist/longmo-stagewise-plugin.umd.js",
  "module": "dist/longmo-stagewise-plugin.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/longmo-stagewise-plugin.es.js",
      "require": "./dist/longmo-stagewise-plugin.umd.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "pnpm run type-check && pnpm run test && pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^23.0.0",
    "sass-embedded": "^1.92.1",
    "typescript": "~5.4.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "vue3",
    "element-selector",
    "dom",
    "highlight",
    "vite",
    "library"
  ],
  "author": "longmo",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0+"
}